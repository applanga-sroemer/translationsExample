GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/Jargon"
LIBRARY_SEARCH_PATHS = "$(PODS_ROOT)/Jargon" "$(PODS_ROOT)/Jargon"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/Jargon"
OTHER_LDFLAGS = $(inherited) -ObjC -l"Jargon" -framework "UIKit"
PODS_ROOT = ${SRCROOT}/Pods